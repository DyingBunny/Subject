#include<iostream>
#include<algorithm>
using namespace std;

void Put(int n,int arr[20000])
{
	int i;
	int j;
	for (i = 1; i <= n; i++)
	{
		int k = i;
		for (j = 2; j*j <= n; j++)
		{
			int s = 0;
			while (k%j == 0)
			{
				s++;
				k = k / j;
			}
			arr[j] = max(arr[j], s);
		}
		if (k > 1)
		{
			arr[k] = max(arr[k], 1);
		}
	}
}

int main()
{
	int n=0;
	int arr[20000] = {0};
	scanf("%d", &n);
	Put(n, arr);
	long long res = 1;
	for (int j = 1; j <= n; j++)
	{
		if (arr[j] > 0)
		{
			for (int m = 1; m <= arr[j]; m++)
			{
				res = res*j%987654321;
			}
		}
	}
	printf("%lld", res);
	system("pause");
	return 0;
}
